--Fixed type
with Ada.Text_IO;   
function Levenshtein(prabhat,pandey : Integer) return Integer is
      Q : Integer;
      type day is delta 5 range 1.5..5.4;
      type night is digits Q RANGE 1..3;
      c: night ;
      b : day;
   begin
      c:=8;
      b := 5.9;
      Q := 1;
      return Q;
   end Levenshtein;
