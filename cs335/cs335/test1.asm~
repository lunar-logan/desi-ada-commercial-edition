.data
a:  .word 1
#b:  .word 1
c:  .word 1
d:  .byte 1
.text
.globl main
main:
li $a0 5
sw $a0 a
lw $a0 a
sw $a0 0($sp)
addiu $sp $sp -4
li $a0 3
lw $t1 4($sp)
mul $a0 $t1 $a0
addiu $sp $sp 4
sw $a0 0($sp)
addiu $sp $sp -4
li $a0 4
lw $t1 4($sp)
add $a0 $t1 $a0
addiu $sp $sp 4
sw $a0 b
li $v0 10
syscall

